{
  "name": "aws-sdk-v3-proxy",
  "version": "0.0.0-development",
  "description": "A wrapper for adding proxy support to AWS SDK v3 clients",
  "main": "./lib/src/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "coverage": "codecov",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/aws-sdk-v3-js-proxy.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Ryan Sonshine",
    "email": "ryansonshine@users.noreply.github.com",
    "url": "https://github.com/ryansonshine"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "aws-sdk",
    "aws-sdk-v3",
    "proxy",
    "proxy-agent"
  ],
  "bugs": {
    "url": "https://github.com/awslabs/aws-sdk-v3-js-proxy/issues"
  },
  "homepage": "https://github.com/awslabs/aws-sdk-v3-js-proxy#readme",
  "dependencies": {
    "proxy-agent": "^5.0.0"
  },
  "peerDependencies": {
    "@aws-sdk/node-http-handler": "^3.15.0",
    "@aws-sdk/protocol-http": "^3.15.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.16.0",
    "@aws-sdk/types": "^3.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.20.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "codecov": "^3.8.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
